- (void) enshelve: (Shelf *) shelve;
- (void) unshelve;



- (void) enshelve: (Shelf *) shelve{
    self.atShelf = shelve;
    self.enshelved = [NSNumber numberWithInt:1];
}

- (void) unshelve{
    self.atShelf = nil;
    self.enshelved = [NSNumber numberWithInt:0];
}






- (NSSet *) reportAllBooks;

- (NSSet *) reportAllBooks {
    return self.books;
}